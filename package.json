{
  "name": "agromarket",
  "version": "1.0.0",
  "description": "agromarket",
  "main": "index.js",
  "private": true,
  "scripts": {
    "start": "node index.js",
    "server": "set DEBUG=agromarket:* & nodemon ./bin/www",
    "client": "npm run start --prefix client",
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "db:reset": "sequelize db:migrate:undo:all && sequelize db:migrate && sequelize db:seed:all",
    "db:recreate": "sequelize db:drop && sequelize db:create",
    "db:hard-reset": "npm run db:recreate && npm run db:reset"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/diginfinity/agromarket.git"
  },
  "author": "digital infinity",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/diginfinity/agromarket/issues"
  },
  "homepage": "https://github.com/diginfinity/agromarket#readme",
  "dependencies": {
    "@hapi/joi": "^17.1.1",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "debug": "^4.1.1",
    "express": "^4.17.1",
    "express-joi-validation": "^4.0.3",
    "express-rate-limit": "^5.1.1",
    "helmet": "^3.22.0",
    "joi": "^14.3.1",
    "morgan": "^1.10.0",
    "mysql2": "^2.1.0",
    "nodemailer": "^6.4.6",
    "sequelize": "^5.21.6"
  },
  "devDependencies": {
    "@types/express": "^4.17.5",
    "@types/node": "^13.11.1",
    "@types/react": "^16.9.33",
    "@types/sequelize": "^4.28.8",
    "@types/supertest": "^2.0.8",
    "concurrently": "^5.1.0",
    "jest": "^25.3.0",
    "nodemon": "^2.0.2",
    "sequelize-cli": "^5.5.1",
    "supertest": "^4.0.2"
  }
}
